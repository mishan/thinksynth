# $Id$

dsp_path := @dsp_path@
default: all

include ../build.mk

# Dummy target
all:

distclean::
	rm -f Makefile

clean::

install:
	mkdir -p $(DESTDIR)$(dsp_path)
	for dsp in $(shell find . -name '*.dsp'); do \
		mkdir -p $(DESTDIR)$(dsp_path)/`dirname "$$dsp"`; \
		install -m 0644 $$dsp $(DESTDIR)$(dsp_path)/$$dsp; \
	done


uninstall:
	rm -rf $(DESTDIR)$(dsp_path)
	-rmdir $(dir $(DESTDIR)$(dsp_path))
