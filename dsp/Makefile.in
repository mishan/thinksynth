# $Id$

dsp_path := @dsp_path@
default: all

include ../build.mk

# Dummy target
all:

distclean::
	rm -f Makefile

clean::

install:
	mkdir -p $(DESTDIR)$(dsp_path)
	tar -cf - `find -name '*.dsp'` | tar -C $(DESTDIR)$(dsp_path) -xf -

uninstall:
	rm -rf $(DESTDIR)$(dsp_path)
	-rmdir $(dir $(DESTDIR)$(dsp_path))
