# $Id$

patch_path := @patch_path@
default: all

include ../build.mk

# Dummy target
all:

distclean::
	rm -f Makefile

clean::

install:
	mkdir -p $(DESTDIR)$(patch_path)
	tar -cf - `find -name '*.patch'` | tar -C $(DESTDIR)$(patch_path) -xf -

uninstall:
	rm -rf $(DESTDIR)$(patch_path)
	-rmdir $(dir $(DESTDIR)$(patch_path))
