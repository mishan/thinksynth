# $Id: Makefile.in,v 1.39 2004/05/26 06:00:24 misha Exp $

BINARY = thinksynth

bindir=@bindir@
prefix=@prefix@

all: thinksynth


include ../build.mk

PICFLAGS =
INCLUDES = -I.. -I../libthink @GTKMM_CFLAGS@
LDFLAGS += @LIBS@ @GTKMM_LIBS@ @GTHREAD_LIBS@ @ASOUND_LIBS@ -L../libthink -lthink -ljack

# objects not compiled anymore:
# thOSSAudio.o thWav.o

MAINOBJS = main.o ui.o $(WIDGETOBJS) thfALSAAudio.o thfALSAMidi.o thfJackAudio.o thfPrefs.o
MONOMAINOBJS = monotest.o thALSAAudio.o thWav.o
WIDGETOBJS = MainSynthWindow.o PatchSelWindow.o Keyboard.o KeyboardWindow.o

thinksynth: $(MAINOBJS)
	$(CXX) $(CXXFLAGS) -o $@ $(MAINOBJS) $(LDFLAGS)  -lthink -L.

monotest: $(MONOMAINOBJS)
	$(CXX) $(CXXFLAGS) -o $@ $(MONOMAINOBJS) $(LDFLAGS) -lthink -L.

clean::
	rm -f thinksynth
	rm -f monotest

distclean::
	rm -f Makefile

install:
	mkdir -p $(DESTDIR)$(bindir)
	cp -f thinksynth $(DESTDIR)$(bindir)

uninstall:
	rm -f $(DESTDIR)$(bindir)/thinksynth
