# $Id: Makefile.in,v 1.24 2004/03/24 09:58:32 misha Exp $

BINARY = thinksynth monotest

bindir=@bindir@
prefix=@prefix@

all: thinksynth monotest

include ../build.mk

PICFLAGS = -fPIC
INCLUDES = -I.. -I../libthink `pkg-config gtkmm-2.0 --cflags`
LDFLAGS += @LIBS@ -L../libthink -lthink

MAINOBJS = main.o ui.o
MONOMAINOBJS = monotest.o

thinksynth: $(MAINOBJS)
	$(CXX) $(CXXFLAGS) -o $@ $(MAINOBJS) $(LDFLAGS) `pkg-config gtkmm-2.0 gthread-2.0 --libs` -lthink -L.

monotest: $(MONOMAINOBJS)
	$(CXX) $(CXXFLAGS) -o $@ $(MONOMAINOBJS) $(LDFLAGS) -lthink -L.

clean::
	rm -f thinksynth
	rm -f monotest

distclean::
	rm -f Makefile

install:
	mkdir -p $(DESTDIR)$(bindir)
	cp -f thinksynth $(DESTDIR)$(bindir)

uninstall:
	rm -f $(DESTDIR)$(bindir)/thinksynth
