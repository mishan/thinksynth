# $Id: Makefile.in,v 1.23 2004/02/20 08:00:56 ink Exp $

BINARY = thinksynth monotest

bindir=@bindir@
prefix=@prefix@

all: thinksynth monotest

include ../build.mk

PICFLAGS = -fPIC
INCLUDES = -I.. -I../libthink
LDFLAGS += @LIBS@ -L../libthink -lthink

MAINOBJS = main.o
MONOMAINOBJS = monotest.o

thinksynth: $(MAINOBJS)
	$(CXX) $(CXXFLAGS) -o $@ $(MAINOBJS) $(LDFLAGS) -lthink -L.

monotest: $(MONOMAINOBJS)
	$(CXX) $(CXXFLAGS) -o $@ $(MONOMAINOBJS) $(LDFLAGS) -lthink -L.

clean::
	rm -f thinksynth
	rm -f monotest

distclean::
	rm -f Makefile

install:
	mkdir -p $(DESTDIR)$(bindir)
	cp -f thinksynth $(DESTDIR)$(bindir)

uninstall:
	rm -f $(DESTDIR)$(bindir)/thinksynth
