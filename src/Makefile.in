# $Id: Makefile.in,v 1.22 2004/02/05 07:07:46 misha Exp $

BINARY = thinksynth

bindir=@bindir@
prefix=@prefix@

all: thinksynth

include ../build.mk

PICFLAGS = -fPIC
INCLUDES = -I.. -I../libthink
LDFLAGS += @LIBS@ -L../libthink -lthink

MAINOBJS = main.o

thinksynth: $(MAINOBJS)
	$(CXX) $(CXXFLAGS) -o $@ $(MAINOBJS) $(LDFLAGS) -lthink -L.

clean::
	rm -f thinksynth

distclean::
	rm -f Makefile

install:
	mkdir -p $(DESTDIR)$(bindir)
	cp -f thinksynth $(DESTDIR)$(bindir)

uninstall:
	rm -f $(DESTDIR)$(bindir)/thinksynth
