# $Id: Makefile.in,v 1.36 2004/04/15 07:41:17 misha Exp $

BINARY = thinksynth

bindir=@bindir@
prefix=@prefix@

all: thinksynth


include ../build.mk

PICFLAGS =
INCLUDES = -I.. -I../libthink @GTKMM_CFLAGS@
LDFLAGS += @LIBS@ @GTKMM_LIBS@ @GTHREAD_LIBS@ -L../libthink -lthink -ljack

MAINOBJS = main.o ui.o prefs.o $(WIDGETOBJS) thfALSAMidi.o thfJackAudio.o thALSAAudio.o thOSSAudio.o thWav.o
MONOMAINOBJS = monotest.o thALSAAudio.o thWav.o
WIDGETOBJS = MainSynthWindow.o PatchSelWindow.o Keyboard.o KeyboardWindow.o

thinksynth: $(MAINOBJS)
	$(CXX) $(CXXFLAGS) -o $@ $(MAINOBJS) $(LDFLAGS)  -lthink -L.

monotest: $(MONOMAINOBJS)
	$(CXX) $(CXXFLAGS) -o $@ $(MONOMAINOBJS) $(LDFLAGS) -lthink -L.

clean::
	rm -f thinksynth
	rm -f monotest

distclean::
	rm -f Makefile

install:
	mkdir -p $(DESTDIR)$(bindir)
	cp -f thinksynth $(DESTDIR)$(bindir)

uninstall:
	rm -f $(DESTDIR)$(bindir)/thinksynth
