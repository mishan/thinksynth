all: gth.a

include ../../build.mk

PICFLAGS =
LDFLAGS += @LIBS@ @GTKMM_LIBS@ @GTHREAD_LIBS@ -L../../libthink -lthink @JACK_LIBS@ @ALSA_LIBS@
INCLUDES = -I../.. -I.. -I../../libthink @GTKMM_CFLAGS@ @JACK_CFLAGS@ @ALSA_CFLAGS@
WIDGETOBJS = AboutBox.o ArgTable.o MainSynthWindow.o MidiMap.o PatchSelWindow.o Keyboard.o KeyboardWindow.o

gth.a: $(WIDGETOBJS)
	ar crsv $@ $(WIDGETOBJS)

clean::
	rm -f gth.a

distclean::
	rm -f Makefile
